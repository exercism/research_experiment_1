{
  "solution_files": ["Csharp2A.cs"],
  "test_file": "Csharp2ATest.cs",
  "tests": [
    {
      "name": "SentenceTest.OneWordWithOneVowel",
      "cmd": "Sentence.WordWithMostVowels(\"a\")",
      "expected": "\"a\""
    },
    {
      "name": "SentenceTest.OneWordWithOneVowelAndOneConsonant",
      "cmd": "Sentence.WordWithMostVowels(\"at\")",
      "expected": "\"at\""
    },
    {
      "name": "SentenceTest.OneWordWithTwoVowelsAndOneConsonant",
      "cmd": "Sentence.WordWithMostVowels(\"tea\")",
      "expected": "\"tea\""
    },
    {
      "name": "SentenceTest.LongestWordIsNotWordWithMostVowels",
      "cmd": "Sentence.WordWithMostVowels(\"the area of a circle\")",
      "expected": "\"area\""
    },
    {
      "name": "SentenceTest.LastWordIsWordWithMostVowels",
      "cmd": "Sentence.WordWithMostVowels(\"extremely fresh vegetables\")",
      "expected": "\"vegetables\""
    },
    {
      "name": "SentenceTest.MixedCasing",
      "cmd": "Sentence.WordWithMostVowels(\"TEST YOUR MIGHT\")",
      "expected": "\"YOUR\""
    },
    {
      "name": "SentenceTest.IgnoreWordWithoutVowels",
      "cmd": "Sentence.WordWithMostVowels(\"great rhythm\")",
      "expected": "\"great\""
    },
    {
      "name": "SentenceTest.TieBreakerWithEquallyLongWords",
      "cmd": "Sentence.WordWithMostVowels(\"her old dad\")",
      "expected": "\"old\""
    },
    {
      "name": "SentenceTest.TieBreakerWithFirstWordBeingWinner",
      "cmd": "Sentence.WordWithMostVowels(\"evening Aeon multiple\")",
      "expected": "\"evening\""
    },
    {
      "name": "SentenceTest.TieBreakerWithLastWordBeingWinner",
      "cmd": "Sentence.WordWithMostVowels(\"a bear was seen on the plains\")",
      "expected": "\"plains\""
    }
  ]
}
