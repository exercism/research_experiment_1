{
  "files": { "solution": ["Csharp1A.cs"], "test": ["Csharp1ATest.cs"] },
  "tests": [
    {
      "test": "SentenceTest.OneWordWithOneVowel",
      "name": "One word with one vowel",
      "cmd": "Sentence.WordWithMostVowels(\"a\")",
      "expected": "\"a\""
    },
    {
      "test": "SentenceTest.OneWordWithOneVowelAndOneConsonant",
      "name": "One word with one vowel and one consonant",
      "cmd": "Sentence.WordWithMostVowels(\"oh\")",
      "expected": "\"oh\""
    },
    {
      "test": "SentenceTest.OneWordWithTwoVowelsAndOneConsonant",
      "name": "One word with two vowels and one consonant",
      "cmd": "Sentence.WordWithMostVowels(\"tea\")",
      "expected": "\"tea\""
    },
    {
      "test": "SentenceTest.LongestWordIsNotWordWithMostVowels",
      "name": "Longest word is not word with most vowels",
      "cmd": "Sentence.WordWithMostVowels(\"the area of a circle\")",
      "expected": "\"area\""
    },
    {
      "test": "SentenceTest.LastWordIsWordWithMostVowels",
      "name": "Last word is word with most vowels",
      "cmd": "Sentence.WordWithMostVowels(\"do you fancy cooking\")",
      "expected": "\"cooking\""
    },
    {
      "test": "SentenceTest.MixedCasing",
      "name": "Mixed casing",
      "cmd": "Sentence.WordWithMostVowels(\"TEST YOUR MIGHT\")",
      "expected": "\"YOUR\""
    },
    {
      "test": "SentenceTest.IgnoreWordWithoutVowels",
      "name": "Ignore word without vowels",
      "cmd": "Sentence.WordWithMostVowels(\"fantastic rhythm guitarist\")",
      "expected": "\"guitarist\""
    },
    {
      "test": "SentenceTest.WordWithAllVowels",
      "name": "Word with all vowels",
      "cmd": "Sentence.WordWithMostVowels(\"Mountain Sequoia Trees\")",
      "expected": "\"Sequoia\""
    }
  ]
}
